<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Action for image view -->
        <record id="action_result_image_view2" model="ir.actions.client">
            <field name="name">Result Images</field>
            <field name="res_model">medical.patient</field>
            <field name="tag">result_images</field>
        </record>
        <!--Dental Chart Opening Action -->
        <record id="action_open_dental_chart" model="ir.actions.client">
            <field name="name">Dental Chart</field>
            <field name="tag">dental_chart</field>
        </record>

        <record id="view_order_form" model="ir.ui.view">
            <field name="name">sale.order.form</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name = 'partner_id']" position="after">
                    <group colspan="4" col="4" >
                        <field name="clinic"/>
                        <field name="doctor"/>
<!--                        <field name="insurance_company"/>-->
                        <field name="patient_id" readonly="1"/>
                    </group>
                </xpath>
            </field>
        </record>

        <!--Account Invoice -->
        <record id="view_move_form" model="ir.ui.view">
            <field name="name">account.invoice.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name = 'company_currency_id']" position="after">
                    <group col="4" >
                        <field name="clinic" />
                        <field name="doctor"/>
                        <field name="insurance_company"/>
                        <field name="patient_id" readonly="1"/>
                    </group>
                </xpath>
            </field>
        </record>

        <!-- medicaments -->
        <record id="medical_medicament_view" model="ir.ui.view">
            <field name="name">Medicament</field>
            <field name="model">medical.medicament</field>
            <field name="arch" type="xml">
                <form string="medicaments">
                    <sheet>
                        <notebook>
                            <page string="Main Info">
                                <group col="2">
                                    <group string="MAIN INFO">
                                        <field context="{'default_is_medicament': 1,'search_default_is_medicament': 1,'search_default_type': 'service','default_type': 'service'}" name="product_medicament_id" required="1"/>
                                        <field name="category"/>
                                        <field name="therapeutic_action"/>
                                        <field name="active_component"/>
                                    </group>
                                    <group string="FINANCIAL INFO">
                                        <field name="price" readonly="1"/>
                                        <field name="qty_available" readonly="1"/>
                                        <field name="indications"/>
                                    </group>
                                </group>
                            </page>
                            <page string="Presentation">
                                <field name="presentation"/>
                            </page>
                            <page string="Composition">
                                <field name="composition"/>
                            </page>
                            <page string="Dosage">
                                <group col="4" colspan="4">
                                    <field name="dosage"/>
                                    <field name="overdosage"/>
                                </group>
                            </page>
                            <page string="Pregnancy / Lactancy">
                                <group col="4" colspan="4">
                                    <field name="pregnancy_warning"/>
                                    <field name="pregnancy_category"/>
                                    <field name="pregnancy"/>
                                </group>
                            </page>
                            <page string="Adverse Reactions">
                                <field name="adverse_reaction"/>
                            </page>
                            <page string="Storage Conditions">
                                <field name="storage"/>
                            </page>
                            <page string="Extra Info">
                                <field name="notes"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="medical_medicament_tree" model="ir.ui.view">
            <field name="name">Medicament list</field>
            <field name="model">medical.medicament</field>
            <field name="arch" type="xml">
                <tree string="medicament">
                    <field name="product_medicament_id"/>
                    <field name="active_component"/>
                    <field name="category"/>
                    <field name="qty_available" readonly="1"/>
                    <field name="therapeutic_action"/>
                    <field name="pregnancy_warning"/>
                    <field name="price"/>
                </tree></field>
        </record>
        <record id="medical_action_tree_medicament" model="ir.actions.act_window">
            <field name="name">Medicaments</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">medical.medicament</field>
            <field name="view_id" ref="medical_medicament_tree"/>
        </record>

        <record id="medical_action_report_medicament" model="ir.actions.act_window">
            <field name="name">Medicament List</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">medical.medicament</field>
            <field name="view_id" ref="medical_medicament_tree"/>
        </record>
         <record id="view_medical_medicament_filter" model="ir.ui.view">
            <field name="name">medical.medicament.select</field>
            <field name="model">medical.medicament</field>
            <field name="arch" type="xml">
                <search string="Search Medicament">
                    <separator orientation="vertical"/>
                    <field name="product_medicament_id"/>
                    <field name="active_component"/>
                    <field name="therapeutic_action"/>
                    <newline/>
                </search></field>
        </record>
        <!-- Patient Medications -->
        <record id="medical_patient_medication_view" model="ir.ui.view">
            <field name="name">Medication</field>
            <field name="model">medical.patient.medication</field>
            <field name="arch" type="xml">
                <form string="Medication">
                    <group col="4" colspan="4">
                        <field name="medicament" required="1"/>
                        <field name="doctor"/>
                        <field name="is_active"/>
                        <field name="indication"/>
                        <field name="start_treatment" required="1"/>
                        <field name="end_treatment"/>
                        <field name="course_completed"/>
                        <field name="discontinued"/>
                        <field invisible="discontinued == False" name="discontinued_reason" required="discontinued == True"/>
                    </group>
                    <group colspan="4" string="Admnistration">
                        <group col="4" colspan="4">
                            <field name="form"/>
                            <field name="route"/>
                        </group>
                        <group colspan="2" string="Dosage">
                            <group col="4" colspan="4">
                                <field name="dose"/>
                                <field name="dose_unit"/>
                                <field name="qty"/>
                            </group>
                        </group>
                        <group colspan="2" string="Treatment Duration">
                            <group col="4" colspan="4">
                                <field name="duration"/>
                                <field name="duration_period"/>
                            </group>
                        </group>
                        <separator string="Common Dosage"/>
                        <group col="4" colspan="4">
                            <field name="common_dosage"/>
                            <field name="admin_times"/>
                        </group>
                        <separator string="Specific Dosage"/>
                        <group col="4" colspan="4">
                            <field name="frequency"/>
                            <field name="frequency_unit"/>
                        </group>
                    </group>
                    <newline/>
                    <separator string="Notes"/>
                    <newline/>
                    <field colspan="4" name="notes" nolabel="1"/>
                </form></field>
        </record>
        <record id="medical_patient_medication_tree" model="ir.ui.view">
            <field name="name">Medication</field>
            <field name="model">medical.patient.medication</field>
            <field name="arch" type="xml">
                <tree decoration-primary="is_active" string="Medication">
                    <field name="medicament"/>
                    <field name="indication"/>
                    <field name="start_treatment"/>
                    <field name="end_treatment"/>
                    <field name="course_completed"/>
                    <field name="discontinued"/>
                    <field name="is_active"/>
                </tree></field>
        </record>
        <!-- Claim Mananagement -->
        <record id="claim_mgmt_insurance_form_view" model="ir.ui.view">
            <field name="name">Claim Management</field>
            <field name="model">dental.insurance.claim.management</field>
            <field name="arch" type="xml">
                <form string="Claim Management">
                    <group col="4" colspan="4">
                        <field name="name" required="1"/>
                        <field name="claim_date" required="1"/>
                    </group>
                    <group col="4" colspan="4">
                        <field name="insurance_company" required="1"/>
                        <field name="insurance_policy_card"/>
                    </group>
                    <group>
                        <field name="treatment_done"/>
                    </group>
                </form></field>
        </record>
