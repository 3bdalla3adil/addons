        <!-- Insurances -->
        <record id="medical_insurance_view" model="ir.ui.view">
            <field name="name">Insurance</field>
            <field name="model">medical.insurance</field>
            <field name="arch" type="xml">
                <form string="Insurances"><group>
                    <group col="2" colspan="2">
                        <field name="number" required="1"/>
                        <field context="{'default_is_patient': 1, 'search_default_is_patient': 1,
                        'search_default_is_person': 1,'default_is_person': 1}"
                               domain="[('is_patient', '=', True)]" name="res_partner_insurance_id"/>
                    </group>
                    <group col="2" colspan="2">
                        <field name="type"/>
                        <field context="{'default_is_insurance_company': 1,
                         'search_default_is_insurance_company': 1}" name="company_id" required="1"/>
                    </group>
                    <group col="2" colspan="2">
                        <field name="plan_id"/>
                        <field name="category"/>
                    </group>
                    <group>
                        <field name="member_since"/>
                        <field name="member_exp"/>
                    </group>
                    <group>
                        <field col="2" colspan="2" name="notes"/>
                    </group>
                </group>
                </form></field>
        </record>
        <record id="medical_insurance_tree" model="ir.ui.view">
            <field name="name">Insurance List</field>
            <field name="model">medical.insurance</field>
            <field name="arch" type="xml">
