        <!-- PARTNER -->
        <record id="view_partner_form3" model="ir.ui.view">
            <field name="name">res.partner.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <field name="ref" position="replace">

                </field>
            </field>
        </record>

        <record id="view_partner_form1" model="ir.ui.view">
            <field name="name">res.partner.form1</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <notebook >
                    <page string="Medical">
                        <group col="6" colspan="6">
                            <field name="is_patient"/>
                            <field name="is_doctor"/>
                            <field name="is_clinic"/>
                            <field name="is_institution"/>
                            <field name="is_person"/>
                            <field name="is_insurance_company"/>
                            <field name="is_pharmacy"/>
                            <field name="ref" placeholder="ID Number"/>
                        </group>
                        <newline/>
                        <field colspan="4" name="insurance_ids" string="Insurance"/>
                    </page>
                    <page string="Treatment">
<!--                        <field name="treatment_ids" string="Treatments"/>-->
                        <field domain="[('is_treatment','=',True)]" name="treatment_id" string="Treatment"/>
                    </page>
                </notebook>
                <field name="user_id" position="replace" >
                    <field name="user_id" string="Internal User"/>
                    <!-- attrs="{ 'required':[('is_doctor', '=', True)]}" -->
                </field>
            </field>
        </record>

        <record id="view_partner_form2" model="ir.ui.view">
            <field name="name">res.partner.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <field name="name" position="after">
                    <field name="middle_name" placeholder="Middle Name......" />
                    <field name="lastname" placeholder="Last Name......" />
                </field>
